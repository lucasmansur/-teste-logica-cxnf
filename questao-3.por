programa  
{  
    funcao inicio()  
    {  
        logico teste  
        inteiro num   
  
        escreva ("Digite um valor para ser comparado :")      
        leia (num)    
  
        se (num>10)
        escreva ("É maior que 10")
        
        senao    
        escreva ("É menor que 10 ")  
    }  
} 